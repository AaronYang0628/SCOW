    "ChangePasswordSchema": {
      "type": "object",
      "properties": {
        "method": {
          "type": "string",
          "const": "PATCH"
        },
        "body": {
          "type": "object",
          "properties": {
            "oldPassword": {
              "type": "string"
            },
            "newPassword": {
              "type": "string",
              "pattern": "^(?=.*\\d)(?=.*[a-zA-Z])(?=.*[`~!@#\\$%^&*()_+\\-[\\];',./{}|:\"<>?]).{8,}$"
            }
          },
          "required": [
            "oldPassword",
            "newPassword"
          ],
          "additionalProperties": false
        },
        "responses": {
          "type": "object",
          "properties": {
            "204": {
              "type": "null",
              "description": "更改成功"
            },
            "412": {
              "type": "null",
              "description": "密码不正确"
            },
            "501": {
              "type": "null",
              "description": "本功能在当前配置下不可用。"
            }
          },
          "required": [
            "204",
            "412",
            "501"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "method",
        "body",
        "responses"
      ],
      "additionalProperties": false,
      "$id": "ChangePasswordSchema",
